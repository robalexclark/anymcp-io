  },
  "homepage": "https://github.com/xakpc/anymcp-io#readme",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
